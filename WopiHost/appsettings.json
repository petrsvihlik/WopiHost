{
  "Wopi": {
    "UseCobalt": false,
    "StorageProviderAssemblyName": "WopiHost.FileSystemProvider",
    "StorageProviderOptions": {
      "RootPath": ".\\wwwroot\\wopi-docs"
    }
  },
  "server.urls": "wopihost:5000",
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}
